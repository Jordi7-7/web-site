---
 interface Props {
    text:string,
    number:string
 }

 const {text, number} = Astro.props
---

<div class="w-1/3 rounded-[80px] bg-gray-bg flex flex-col gap-4 justify-center items-center py-20">
    <p class="text-primary text-8xl font-medium">{number}</p>
    <p class="text-black text-lg">{text}</p>
</div>